generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Film {
  imdbId  String    @id
  title   String
  plot    String
  year    String
  poster  String
  Meeting Meeting[]
}

model Meeting {
  id     Int      @id @default(autoincrement())
  date   DateTime @unique
  film   Film     @relation(fields: [filmId], references: [imdbId])
  filmId String
  host   String
}
